{
  "version": 2,
  "settings": {
    "layout": {
      "contentSize": "960px",
      "wideSize": "1200px"
    },
    "appearanceTools": true,
    "color": {
      "defaultDuotone": false,
      "defaultGradients": false,
      "defaultPalette": true,
      "palette": [
        {
          "slug": "primary",
          "color": "#00896C",
          "name": "Primary"
        },
        {
          "slug": "secondary",
          "color": "#E47D5C",
          "name": "Secondary"
        },
        {
          "slug": "accent",
          "color": "#555555",
          "name": "Accent"
        },
        {
          "slug": "background",
          "color": "#FFFFFF",
          "name": "Background"
        },
        {
          "slug": "font",
          "color": "#4A4A4A",
          "name": "Font"
        }
      ]
    },
    "spacing": {
      "padding": true,
      "margin": true,
      "blockGap": true,
      "defaultSpacingSizes": false,
      "spacingSizes": [
        {
          "name": "XXS",
          "size": "8px",
          "slug": "20"
        },
        {
          "name": "XS",
          "size": "18px",
          "slug": "30"
        },
        {
          "name": "S",
          "size": "36px",
          "slug": "40"
        },
        {
          "name": "標準",
          "size": "clamp(36px, 1.5rem + 2.5vw, 48px)",
          "slug": "50"
        },
        {
          "name": "L",
          "size": "clamp(48px, 2rem + 3.333vw, 64px)",
          "slug": "60"
        },
        {
          "name": "XL",
          "size": "clamp(64px, 2.625rem + 4.583vw, 86px)",
          "slug": "70"
        },
        {
          "name": "XXL",
          "size": "clamp(86px, 4rem + 4.583vw, 108px)",
          "slug": "80"
        }
      ],
      "units": ["%", "px", "em", "rem", "vh", "vw"]
    },
    "typography": {
      "defaultFontSizes": false,
      "fluid": true,
      "fontFamilies": [
        {
          "fontFamily": "'Noto Sans JP', sans-serif",
          "name": "Primary",
          "slug": "primary",
          "fontFace": [
            {
              "fontFamily": "'Noto Sans JP'",
              "fontWeight": "100 900",
              "fontStyle": "normal",
              "fontDisplay": "swap",
              "src": [
                "file:./assets/fonts/NotoSansJP-VariableFont_wght.woff2",
                "file:./assets/fonts/NotoSansJP-VariableFont_wght.woff"
              ]
            }
          ]
        }
      ],
      "fontSizes": [
        {
          "fluid": false,
          "name": "小",
          "size": "0.75rem",
          "slug": "small"
        },
        {
          "fluid": {
            "max": "1rem",
            "min": "0.875rem"
          },
          "name": "中",
          "size": "0.9375rem",
          "slug": "medium"
        },
        {
          "fluid": {
            "max": "1.375rem",
            "min": "1.125rem"
          },
          "name": "大",
          "size": "1.25rem",
          "slug": "large"
        },
        {
          "fluid": {
            "max": "1.75rem",
            "min": "1.5rem"
          },
          "name": "特大",
          "size": "1.625rem",
          "slug": "x-large"
        },
        {
          "fluid": {
            "max": "2.25rem",
            "min": "2rem"
          },
          "name": "超特大",
          "size": "2.125rem",
          "slug": "xx-large"
        }
      ],
      "writingMode": true
    },
    "useRootPaddingAwareAlignments": true,
    "custom": {
      "radious": {
        "button": "0",
        "image": "0"
      },
      "gap": "clamp(1rem, 3.333vw, 2rem)"
    }
  },
  "styles": {
    "blocks": {
      "core/button": {
        "border": {
          "radius": "var(--wp--custom--radious--button)"
        }
      },
      "core/buttons": {
        "spacing": {
          "blockGap": "var(--wp--custom--gap)"
        }
      },
      "core/image": {
        "border": {
          "radius": "var(--wp--custom--radious--image)"
        }
      },
      "core/columns": {
        "spacing": {
          "blockGap": "var(--wp--custom--gap)"
        }
      },
      "core/list": {
        "css": "& li{margin-top: 0.5rem;}"
      },
      "core/navigation": {
        "elements": {
          "link": {
            ":hover": {
              "typography": {
                "textDecoration": "underline"
              }
            },
            "typography": {
              "textDecoration": "none"
            }
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)"
        }
      },
      "core/separator": {
        "border": {
          "color": "currentColor",
          "style": "solid",
          "width": "0 0 1px 0"
        },
        "color": {
          "text": "var(--wp--preset--color--font)"
        }
      }
    },
    "color": {
      "background": "var(--wp--preset--color--background)",
      "text": "var(--wp--preset--color--font)"
    },
    "elements": {
      "button": {
        ":focus": {
          "color": {
            "background": "color-mix(in srgb, var(--wp--preset--color--accent) 85%, transparent)",
            "text": "var(--wp--preset--color--background)"
          }
        },
        ":hover": {
          "color": {
            "background": "color-mix(in srgb, var(--wp--preset--color--accent) 85%, transparent)",
            "text": "var(--wp--preset--color--background)"
          }
        },
        "color": {
          "background": "var(--wp--preset--color--accent)",
          "text": "var(--wp--preset--color--background)"
        },
        "border": {
          "radius": "var(--wp--custom--radious--button)"
        },
        "spacing": {
          "padding": {
            "bottom": "0.5rem",
            "left": "1rem",
            "right": "2.75rem",
            "top": "0.5rem"
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)"
        }
      },
      "caption": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)",
          "lineHeight": "1.4"
        }
      },
      "h1": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--xx-large)"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-large)"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--medium)"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)",
          "letterSpacing": "0.5px"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)",
          "letterSpacing": "1.4px"
        }
      },
      "heading": {
        "typography": {
          "fontWeight": "700",
          "letterSpacing": "-0.1px",
          "lineHeight": "1.125"
        }
      },
      "link": {
        ":hover": {
          "typography": {
            "textDecoration": "none"
          }
        },
        "color": {
          "text": "currentColor"
        }
      }
    },
    "spacing": {
      "blockGap": "var(--wp--custom--gap)",
      "padding": {
        "left": "var(--wp--custom--gap)",
        "right": "var(--wp--custom--gap)"
      }
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--primary)",
      "fontSize": "var(--wp--preset--font-size--medium)",
      "fontWeight": "400",
      "lineHeight": "1.6"
    }
  },
  "templateParts": [
    {
      "area": "header",
      "name": "header",
      "title": "ヘッダー"
    },
    {
      "area": "footer",
      "name": "footer",
      "title": "フッター"
    }
  ]
}
